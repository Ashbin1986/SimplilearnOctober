
@{
    ViewBag.Title = "UploadFile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Upload File</h2>

@using (Html.BeginForm("UploadFileData", "FileUpload", FormMethod.Post, new {enctype = "multipart/form-data"}))
{
    <div class="form-group">
        @Html.TextBox("fileup", "", new { type = "file" })
    </div>
    <div class="form-group">
        <input type="submit" value="upload" />
    </div>
}

